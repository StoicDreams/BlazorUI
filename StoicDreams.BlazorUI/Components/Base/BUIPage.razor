@inherits BUICore

@code {
	protected string Title { get => GetAppState<string>(AppStateDataTags.PageTitle, () => string.Empty); set => SetAppState(AppStateDataTags.PageTitle, value); }
	protected string PageKey { get; set; } = string.Empty;

	protected override Task OnInitializedAsync()
	{
		PageKey = GetAppState<string>(AppStateDataTags.CurrentPage, () => string.Empty);
		SetAppStateWithTrigger(AppStateDataTags.IsLoadingPage, false);
		return base.OnInitializedAsync();
	}
}
@inherits BUICore

@code {
	protected string Title { get => GetState<string>(AppStateDataTags.PageTitle, () => string.Empty); set => SetState(AppStateDataTags.PageTitle, value); }

	protected override Task OnInitializedAsync()
	{
		SetStateWithTrigger(AppStateDataTags.IsLoadingPage, false);
		return base.OnInitializedAsync();
	}
}
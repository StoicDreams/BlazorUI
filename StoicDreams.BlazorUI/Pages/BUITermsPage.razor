@inherits BUIPage
@page "/terms"

<DynamicComponent Type="GetContentType" />

@code {
	private Type GetContentType => GetState<Type>(AppStateDataTags.TermsPageContent) ?? typeof(BUITermsPageContent);

	protected override Task OnInitializedAsync()
	{
		Title = "Terms & Conditions";
		return base.OnInitializedAsync();
	}
}
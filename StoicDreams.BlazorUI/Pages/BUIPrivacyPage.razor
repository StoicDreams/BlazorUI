@inherits BUIPage
@page "/privacy"

<DynamicComponent Type="GetContentType" />

@code {
	private Type GetContentType => GetAppState<Type>(AppStateDataTags.PrivacyPageContent) ?? typeof(BUIPrivacyPageContent);

	protected override Task OnInitializedAsync()
	{
		Title = "Privacy Policy";
		return base.OnInitializedAsync();
	}
}
